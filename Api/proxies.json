{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "AzureRetailPrices": {
      "matchCondition": {
        "methods": [
          "GET"
        ],
        "route": "/api/prices"
      },
      "backendUri": "https://prices.azure.com/api/retail/prices",
      "responseOverrides": {
        "response.headers.Cache-Control": "public, max-age=3600",
        "response.headers.pragma": "ignore"
      }
    }
  }
}