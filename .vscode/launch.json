{
  "version": "0.2.0",
  "compounds": [
    {
      "name": "Static Web App",
      "configurations": [
        "BlazorClient"
      ],
      // "preLaunchTask": "func: host start",
      "preLaunchTask": "staticwebapp",
      "presentation": {
        "hidden": false,
        "group": "",
        "order": 1
      }
    },
  ],
  "configurations": [
    {
      "name": "BlazorClient",
      "type": "blazorwasm",
      "request": "launch",
      "cwd": "${workspaceFolder}/Client",
      "browser": "edge",
      "presentation": {
        "hidden": false,
        "group": "server",
        "order": 1
      }
    },
    {
      "name": "AzureFunctionsPowershell",
      "type": "PowerShell",
      "request": "attach",
      "customPipeName": "AzureFunctionsPSWorker",
      "runspaceId": 1,
      "preLaunchTask": "func: host start"
    }
  ]
}